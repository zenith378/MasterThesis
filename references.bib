@article{HULSBERGEN2009471,
title = {The global covariance matrix of tracks fitted with a Kalman filter and an application in detector alignment},
journal = {Nuclear Instruments and Methods in Physics Research Section A: Accelerators, Spectrometers, Detectors and Associated Equipment},
volume = {600},
number = {2},
pages = {471-477},
year = {2009},
